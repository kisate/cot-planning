(unstack a d)
(put-down a)
(unstack d b)
(put-down d)
(unstack b g)
(put-down b)
(unstack g c)
(put-down g)
(unstack c f)
(put-down c)
(pick-up e)
(stack e c)
(pick-up a)
(stack a e)
(pick-up g)
(stack g a)
(pick-up b)
(stack b g)
