(put-down b)
(unstack a g)
(stack a b)
(unstack g f)
(put-down g)
(unstack f e)
(put-down f)
(unstack e c)
(put-down e)
(unstack c d)
(put-down c)
(pick-up e)
(stack e d)
(pick-up f)
(stack f e)
