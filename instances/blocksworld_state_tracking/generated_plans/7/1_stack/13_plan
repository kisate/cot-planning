(put-down a)
(unstack b e)
(put-down b)
(unstack e c)
(put-down e)
(unstack c g)
(put-down c)
(unstack g f)
(put-down g)
(unstack f d)
(put-down f)
(pick-up d)
(stack d f)
(pick-up e)
(stack e d)
(pick-up a)
(stack a e)
